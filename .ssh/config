Host *
   User tom
   ControlMaster auto
   ControlPath ~/.ssh/master-%r@%h:%p
   VisualHostKey yes
   VerifyHostKeyDNS yes
   StrictHostKeyChecking yes
   PasswordAuthentication no
   IdentitiesOnly yes

Host *.jitter.local
   HostName kali.jitter.local
   IdentityFile ~/.ssh/svaha
   VisualHostKey no

Host svaha.jitter.eu
   VerifyHostKeyDNS no
   IdentityFile ~/.ssh/svaha

Host github.com
   IdentityFile ~/.ssh/github
