set -e

### keychain
command -v keychain >/dev/null && keychain --list

### use autobitch if available
if test -f "$(brew --prefix autobitch)/share/autobitch.sh"; then
    use_auto "$1"
    use_auto_python "$1"
    use_auto_ruby "$1"
fi

### add executables to PATH
test -d 'bin' && PATH_add 'bin'
