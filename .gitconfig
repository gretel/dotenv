[alias]
  a = add
  aa = add -A
  b = branch
  bl = blame --line-porcelain --
  bv = branch -v
  c = commit
  ca = commit --amend
  can = commit --amend --no-edit
  cf = diff --diff-filter=U
  cl = clone --recurse-submodules --depth 10
  cs = clone --recurse-submodules --depth 1
  cm = commit -m
  cn = commit --no-edit
  co = checkout
  d = diff
  di = diff
  ds = diff --staged
  f = fetch
  fs = git fetch --recurse-submodules
  i = "!gi() { curl -L -s https://www.gitignore.io/api/$@ ;}; gi"
  l = log
  last = diff --cached HEAD^
  lg = log --graph --decorate --pretty=oneline --abbrev-commit
  lgr = log --graph -n 20 --format='format:%C(blue)%h%C(reset) %C(black)%an%C(reset) %s %C(black bold)%cr%C(reset)%C(auto)%d%C(reset)' --date-order --tags --all
  lp = log --pretty=oneline --stat --decorate
  m = merge
  p = push
  pl = pull
  plr = pull --rebase
  pls = "!f() { git stash; git pull $@; git stash apply; }; f"
  plup = "!git fetch origin -v; git fetch upstream -v; git merge upstream/master"
  pu = push
  r = reset
  rb = rebase
  rbi = rebase -i
  rh = reset HEAD
  rt = remote
  rtv = remote -v
  s = status -s
  sb = status -sb
  sg = diff --staged
  st = status
  t = tag
  tl = tag -l
  unadd = reset HEAD
  unalias = "!f() { git config --global --unset alias.$1; }; f"
  ux = update-index --chmod=+x
[branch]
  autosetupmerge = true
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
  ui = auto
[commit]
  short = true
[core]
  editor = subl -w -n
  pager = delta --dark
  whitespace = trailing-space,space-before-tab
  excludesfile = /Users/tom/.gitignore_global
  autocrlf = input
  ignorecase = false
  safecrlf = warn
[diff]
  algorithm = patience
  renames = copies
  mnemonicprefix = true
  tool = difftools
[fetch]
  fsckobjects = true
  prune = true
[github]
  user = gretel
[grep]
  extendRegexp = true
[help]
  autocorrect = 1
[pager]
  blame = less -S
[push]
  default = current
[receive]
  fsckObjects = true
[rerere]
  enabled = true
[tag]
  sort = version:refname
[transfer]
  fsckobjects = true
[user]
  email = code@jitter.eu
  name = gretel
[gpg]
  program = gpg
[init]
  templatedir = ~/.git-templates
[advice]
  pushNonFastForward = false
  statusHints = false
[merge]
  stat = true
  tool = difftools
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[mergetool "difftools"]
  trustExitCode = false
[mergetool]
  keepBackup = false
[credential]
  helper = osxkeychain
